function update_menu()
	local menu_actions={
		click_select,
		click_mine,
		click_chop,
		click_crop,
		click_attack,
	}

	if stat(34) == 1 then
		for i=1, 5 do
			local x, y = 8*(i-1)+camera_x_offset, camera_y_offset
			if is_hovering(x, y) then
				menu_actions[i]()
			end
		end
	end
end



function draw_menu()
	for i=25, 31 do
		local x, y = 8*(i-25)+camera_x_offset, camera_y_offset
		local sp = is_hovering(x, y) and i+16 or i
		spr(sp,x,y)
	end
end
