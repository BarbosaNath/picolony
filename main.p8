-- init --
function _init()
	poke(0x5f2d, 1) -- mouse
	generate_world()
	
	mouse_x=0
	mouse_y=0
	
	camera_spd=2
	
	camera_x=0
	camera_y=0
	
	camera_x_offset=peek2(0x5f28)
	camera_y_offset=peek2(0x5f2a)
	
	selected = nil
	
	cursor_sp = 15
	cursor_mode = 'select'
	
	cursor_actions = {
		select=select_entity,
		mine=empty_fn,
		chop=empty_fn,
		crop=empty_fn,
		attack=empty_fn,
		build=empty_fn,
	}
	
	for i=1, 10 do
		person:generate()
	end
end


-- update --
function _update()
	for entt in all(entities) do
		entt:update()
	end
	
	cursor_actions[cursor_mode]()
	
	camera_x_offset=peek2(0x5f28)
	camera_y_offset=peek2(0x5f2a)
	
	mouse_x=stat(32) + camera_x_offset
	mouse_y=stat(33) + camera_y_offset
	
	update_menu()
	
	-- camera movement
	if(btn(⬅️))camera_x-=1*camera_spd
	if(btn(➡️))camera_x+=1*camera_spd
	if(btn(⬆️))camera_y-=1*camera_spd
	if(btn(⬇️))camera_y+=1*camera_spd
end


-- draw --
function _draw()
 cls()

	map(0,0,0,0,size-20,size-20)
	
	draw_on_top = {}
	for entt in all(entities) do
		if (fget(entt.sp,3)) then
			add(draw_on_top, entt)
		else
			entt:draw()
		end
	end
	
	for entt in all(draw_on_top) do
		entt:draw()
	end
	
	draw_selection()
	
	if selected~=nil then
		selected:draw_info()
	end
	
	draw_menu()
	draw_cursor()
	
	camera(camera_x, camera_y)
end



function print_entities()
	local a = #entities*-7
	for e in all(entities) do
		print(dump(e), 0,a,7)
		a+=7
	end
	
	print(#entities,0,0,10)
end







