function pop_left(t)
	local value = t[1]
	
	deli(t, 1)
	
	return value
end


function is_empty(t)
	return #t == 0
end

function is_tbl(t)
	return	type(t) == 'table'
end

function dump(o)
   if is_tbl(o) then
      local s = '{ '
      for k,v in pairs(o) do
         if type(k) ~= 'number' then k = '"'..k..'"' end
         s = s..k..': ' .. dump(v) .. ','
      end
      return s .. '} '
   else
      return tostring(o)
   end
end

function empty_fn()end
