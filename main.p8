	nacts={}, --next_actions
	lacts={}, --last_actions
	sp=1,
	variant=1,
	inventory={},
	
	humor={
		happiness=5,
		sadness=0,
		hatred=0,
		love=0,
		boredon=0,
	},
	
	humor_text='neutral',
	humor_color=6,
	
	relations={},
	
	
	-- render person information --
	draw_info=function(_ENV)
		draw_info_menu({
			{t=name, c=7},
			{t=action, c=5},
			{t=id, c=5},
		},	{
			{t='happiness: '..humor.happiness, c=10},
	  {t='sadness: '  ..humor.sadness,   c=13},
		 {t='hatred: '   ..humor.hatred,    c=8},
		 {t='love: '     ..humor.love,      c=14},
		 {t='boredon: '  ..humor.boredon,   c=6},
		})
	end,
	
	
	get_next_or_idle=function(_ENV, last)
		if #nacts == 0	then
			add(lacts, last)
			action = 'idle'
		else
			action =	pop_left(nacts)
		end
	end,
	
	
	----------- actions -----------
	
	-- idle action --
	idle=function(_ENV)
		if target.timer == -1 then
			target.timer = flr(rnd(40))
		end
		
		target.timer -= 1
	
		if target.timer == 0 then
			add(lacts, 'idle')
			target.timer, action = -1, 'random_action'
		end
	end,
	
	-- move action --
	move=function(_ENV)
		if target.x == x
		and target.y == y then
			get_next_or_idle(_ENV,'move')
			
			sp = variant
			return
		end
	
		sp = ((sp) % 2) + variant
	
		if target.x != x then
			x += target.x > x and 1 or -1
		end
		
		if target.y != y then
			y += target.y > y and 1 or -1
		end
	end,
	
	
	-- select random action --
	random_action=function(_ENV)	
		action = rnd({
			'idle',
			'move',
			'random_move',
		})
	end,
	
	-- select random place to move --
	random_move=function (_ENV)
		local dis_x = flr(rnd(127))-63
		local dis_y = flr(rnd(127))-63
		
		local pos_x,pos_y = x+dis_x,y+dis_y
		
		-- out of map bounds
		if pos_x<0 or pos_x>(size-20)*8
		or pos_y<0 or pos_y>(size-20)*8
		then return end
		
		target.x,target.y=pos_x,pos_y

		-- move only to grass  		
		if (mget((target.x+4)/8, (target.y+4)/8) == 32)
		then action = 'move' end
	end,
	
	
	-- act acording to action --
	act=function(_ENV)
		_ENV[action](_ENV)
	end,
	
	
	------ helper functions -------
	
	-- update --
	update=function(self)
		self:act()
	end,
	
	-- draw --
	draw=function(_ENV)
		spr(sp,x,y)
	end,
	
	-- generate person --
	generate=function(self)
	 local sp = rnd({1,3})
		local person = self:new({
			name=rnd(
				{
					'alex', 'breno', 'claudio',
					'doug', 'eliza', 'francis',
					'guilheme', 'hilda', 'igor',
					'juniper', 'kai', 'lucas',
					'matheus', 'natalia', 
					'olivia', 'penelope', 
					'quentin',	'roger', 'silvia',
					'tulio', 'ursola',
					'vitoria', 'willian',
					'xavier','yasmin','zuko' 
				}
			),
			x=flr(rnd(127)),
			y=flr(rnd(127)),
			sp=sp,
			variant=sp,
			target={x=0,y=0,timer=0}
		})
		
		return person
	end,

})


